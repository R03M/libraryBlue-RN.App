{
  "expo": {
    "name": "libraryBlue",
    "slug": "libraryBlue",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#155a95"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mrBlue.libraryBlue",
      "buildNumber": "1",
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        },
        "NSAllowsArbitraryLoadsForMedia": true
      }
    },
    "android": {
      "package": "com.mrBlue.libraryBlue",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": ["CAMERA_ROLL", "INTERNET"]
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "extra": {
      "eas": {
        "projectId": "d7a12645-0cae-4897-be2a-28fa5f0ab1e0"
      }
    }
  }
}
